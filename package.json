  "license": "MIT",
  "author": {
    "name": "There",
    "email": "support@there.team"
  },
  "contributors": [
    "Mohammad Rajabifard <mo@there.team>"
  ],
  "devDependencies": {
    "@zeit/next-css": "0.1.4",
    "babel-eslint": "8.2.1",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-styled-components": "1.4.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "5.1.3",
    "downshift": "1.28.5",
    "electron": "7.1.7",
    "electron-builder": "22.5.1",
    "electron-builder-squirrel-windows": "20.8.0",
    "electron-notarize": "0.2.0",
    "eslint": "4.16.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.5.0",
    "eslint-plugin-react": "7.5.1",
    "jstimezonedetect": "1.0.6",
    "just-compare": "1.2.1",
    "just-compose": "1.0.6",
    "just-debounce-it": "1.0.1",
    "lint-staged": "9.2.5",
    "luxon": "1.21.1",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "next": "5.1.0",
    "nodemon": "1.14.11",
    "polished": "1.9.0",
    "prettier": "1.19.1",
    "prop-types": "15.6.0",
    "raven-js": "3.22.3",
    "react": "16.4.0",
    "react-animate-height": "0.10.10",
    "react-beautiful-dnd": "6.0.0",
    "react-content-loader": "3.1.1",
    "react-dom": "16.4.0",
    "react-dropzone": "4.2.9",
    "react-measure": "^2.0.0",
    "react-onclickoutside": "6.7.1",
    "socket.io-client": "2.0.4",
    "styled-components": "3.2.3",
    "unsplash-js": "4.8.0",
    "unstated": "1.1.0",
    "urql": "0.3.0-next1",
    "wenk": "1.0.7"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "create-react-context": "0.1.6",
    "dotenv": "4.0.0",
    "electron-debug": "1.5.0",
    "electron-default-menu": "1.0.1",
    "electron-devtools-installer": "2.2.4",
    "electron-is-dev": "0.3.0",
    "electron-log": "2.2.14",
    "electron-next": "3.1.4",
    "electron-react-devtools": "0.5.3",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "1.3.0",
    "electron-updater": "4.2.5",
    "electron-util": "0.6.0",
    "first-run": "1.2.0",
    "fix-path": "2.1.0",
    "graphql-request": "1.8.2",
    "husky": "3.0.5",
    "menubar": "8.0.1",
    "mixpanel": "0.11.0",
    "ms": "2.1.1",
    "node-fetch": "3.1.1",
    "node-machine-id": "1.1.10",
    "patch-package": "6.2.0",
    "qs": "6.5.1",
    "raven": "2.6.4",
    "semver": "5.5.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "build": {
    "buildVersion": "0",
    "appId": "pm.there.desktop",
    "afterSign": "scripts/notarize.js",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "main/static/icons/mac.icns",
      "entitlements": "main/static/entitlements.mac.plist",
      "entitlementsInherit": "main/static/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "hardenedRuntime": true,
      "target": [
        "zip",
        "dmg"
      ],
      "darkModeSupport": true,
      "extendInfo": {
        "LSUIElement": 1,
        "NSUserNotificationAlertStyle": "alert"
      }
    },
    "win": {
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ],
    "appx": {
      "displayName": "There PM",
      "identityName": "11339MoRajabifard.TherePM",
      "applicationId": "MoRajabifard.TherePM",
      "publisher": "CN=937F087C-7B47-48C8-B363-6D0234B7403C",
      "publisherDisplayName": "Mo Rajabifard"
    }
  },
  "scripts": {
    "dev": "electron main",
    "start": "cross-env ONLINE_API=1 electron main",
    "build": "yarn build-renderer && yarn build-app",
    "build-app": "electron-builder",
    "build-renderer": "next build renderer && next export renderer",
    "format": "prettier --write '{renderer,main}/**/*.{js,json}'",
    "test": "yarn test-lint",
    "test-lint": "eslint . --fix",
    "precommit": "lint-staged",
    "postinstall": "patch-package"
  }
}
